local SelectAllBtn = Instance.new("TextButton")
SelectAllBtn.Size = UDim2.new(0.48, 0, 0, 35)
SelectAllBtn.Position = UDim2.new(0, 10, 0, 310)
SelectAllBtn.BackgroundColor3 = Theme.BackgroundLighter
SelectAllBtn.BorderSizePixel = 0
SelectAllBtn.Text = "Select All"
SelectAllBtn.TextColor3 = Theme.Text
SelectAllBtn.TextSize = 13
