SelectAllBtn.ZIndex = 101
SelectAllBtn.Parent = FlingFrame

Instance.new("UICorner", SelectAllBtn).CornerRadius = UDim.new(0, 8)

local DeselectAllBtn = Instance.new("TextButton")
