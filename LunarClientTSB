DeselectAllBtn.ZIndex = 101
DeselectAllBtn.Parent = FlingFrame

Instance.new("UICorner", DeselectAllBtn).CornerRadius = UDim.new(0, 8)

local StartFlingBtn = Instance.new("TextButton")
StartFlingBtn.Size = UDim2.new(0.48, 0, 0, 40)
StartFlingBtn.Position = UDim2.new(0, 10, 1, -55)
StartFlingBtn.BackgroundColor3 = Theme.Success
