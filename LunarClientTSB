local ApplyCorner = Instance.new("UICorner")
ApplyCorner.CornerRadius = UDim.new(0, 8)
ApplyCorner.Parent = ApplyColorBtn

local selectedColor = Color3.fromRGB(128, 128, 128)
local draggingColor = false

local function getColorFromWheel(x, y)
    local centerX = ColorWheel.AbsoluteSize.X / 2
    local centerY = ColorWheel.AbsoluteSize.Y / 2
    local relX = x - centerX
    local relY = y - centerY
    
    local distance = math.sqrt(relX^2 + relY^2)
    local maxDist = math.min(centerX, centerY)
    
    if distance > maxDist then
        relX = relX * (maxDist / distance)
        relY = relY * (maxDist / distance)
        distance = maxDist
    end
    
    local angle = math.atan2(relY, relX)
    local hue = (angle + math.pi) / (2 * math.pi)
    local sat = distance / maxDist
    
    return Color3.fromHSV(hue, sat, 1), relX + centerX, relY + centerY
end

ColorWheel.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingColor = true
        local relX = input.Position.X - ColorWheel.AbsolutePosition.X
        local relY = input.Position.Y - ColorWheel.AbsolutePosition.Y
        local color, selectorX, selectorY = getColorFromWheel(relX, relY)
        selectedColor = color
        ColorPreview.BackgroundColor3 = color
        ColorSelector.Position = UDim2.new(0, selectorX - 9, 0, selectorY - 9)
    end
end)

ColorWheel.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingColor = false
    end
end)

ApplyColorBtn.MouseButton1Click:Connect(function()
    korbloxColor = selectedColor
    ColorPickerFrame.Visible = false
    colorPickerOpen = false
    
    if player.Character and korbloxHeadlessEnabled then
        local rightLeg = player.Character:FindFirstChild("Right Leg") or player.Character:FindFirstChild("RightLeg")
        if rightLeg then
            rightLeg.Color = korbloxColor
        end
    end
end)

